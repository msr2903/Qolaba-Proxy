{
  "name": "qoloba-proxy",
  "version": "1.0.0",
  "description": "OpenAI-compatible proxy server for Qolaba API",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "dev": "node --watch src/index.js",
    "build": "echo 'No build step required for Node.js'",
    "test": "cross-env NODE_ENV=test jest --forceExit",
    "test:e2e": "cross-env NODE_ENV=test NODE_OPTIONS=\"--experimental-vm-modules\" jest --forceExit",
    "test:watch": "cross-env NODE_ENV=test NODE_OPTIONS=\"--experimental-vm-modules\" jest --watch",
    "test:coverage": "cross-env NODE_ENV=test NODE_OPTIONS=\"--experimental-vm-modules\" jest --coverage",
    "lint": "eslint src/ --ext .js",
    "lint:fix": "eslint src/ --ext .js --fix",
    "docker:build": "docker build -t qoloba-proxy .",
    "docker:run": "docker run -p 3000:3000 --env-file .env qoloba-proxy"
  },
  "keywords": [
    "openai",
    "proxy",
    "qolaba",
    "api",
    "llm",
    "chat",
    "streaming"
  ],
  "author": "Qoloba Proxy Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "morgan": "^1.10.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "@jest/globals": "^30.2.0",
    "babel-jest": "^30.2.0",
    "cross-env": "^10.1.0",
    "eslint": "^8.54.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.7.0",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/qoloba-proxy.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/qoloba-proxy/issues"
  },
  "homepage": "https://github.com/your-org/qoloba-proxy#readme"
}
